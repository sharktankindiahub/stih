{
  "name": "stih",
  "version": "1.0.0",
  "description": "STIH - Node.js application with dev, test, and prod environments",
  "main": "src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node src/index.js",
    "test": "cross-env NODE_ENV=test jest --forceExit",
    "prod": "cross-env NODE_ENV=production node src/index.js",
    "start": "npm run dev",
    "dev:watch": "cross-env NODE_ENV=development nodemon src/index.js",
    "test:watch": "cross-env NODE_ENV=test jest --watch"
  },
  "keywords": [
    "STIH"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
